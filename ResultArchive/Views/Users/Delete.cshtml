@model ResultArchive.Models.ApplicationUser

@section LeftSidebar {
    <partial name="~/Views/Partials/_Sidebar.cshtml" />
}
<!-- Page Content-->
<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="mt-0 header-title">Create Sessions</h4>
                        <p class="text-muted mb-4">
                            Here you create <code class="highlighter-rouge">Sessions</code> used in the system.
                        </p>

                        <form method="post" asp-action="DeleteConfirm">
                            <div class="row">
                                <input type="hidden" asp-for="Id" />
                                <div class="col-lg-8">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">First Name</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text" readonly asp-for="FirstName" />
                                            <span asp-validation-for="FirstName" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Last Name </label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text" readonly asp-for="LastName" />
                                            <span asp-validation-for="LastName" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Other Names </label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text" readonly asp-for="OtherNames" >
                                            <span asp-validation-for="OtherNames" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">School Acronym</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text" required asp-for="Email" >
                                            <span asp-validation-for="Email" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>
  

                                <div class="col-lg-4">
                                    <div class="col-md-12">
                                        <div class="alert alert-outline-danger alert-danger-shadow mb-0 fade show" role="alert">

                                            <i class="mdi mdi-trash-can alert-icon"></i>
                                            @ViewBag.Warning
                                        </div>
                                    </div>
                                    <div class="col-md-12 mt-3">
                                        <div class="form-group row">
                                            <button type="submit" class="btn btn-primary ml-2">Delete User</button>

                                        </div>
                                    </div>
                                </div>

                            </div>
                        </form>

                    </div><!--end card-body-->
                </div><!--end card-->
            </div><!--end col-->
        </div><!--end row-->



    </div><!-- container -->

    <partial name="~/Views/Partials/_Footer.cshtml" />
</div>
<!-- end page content -->