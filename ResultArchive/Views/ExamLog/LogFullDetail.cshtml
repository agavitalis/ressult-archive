
@model IEnumerable<ResultArchive.Models.ExamLog>

@section LeftSidebar {
    <partial name="~/Views/Partials/_Sidebar.cshtml" />
}
<!-- Page Content-->
<div class="page-content">

    <div class="container-fluid">
        @foreach (var examlog in Model)
        {
            <div class="row">
                <div class="col-md-12 col-lg-8">
                    <div class="card">
                        <div class="card-body">
                            <h4 class="mt-0 header-title">SN:@examlog.SN </h4>
                            <p class="text-muted">
                                <h4 class="mt-0 header-title">Question: </h4>@examlog.Question
                            </p>
                            <div class="row">
                                <div class="col-md-12 col-lg-6">
                                    <ul class="list-unstyled">
                                        <li class="mb-5">
                                            <h6 class="">Reference Text</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Refrence_Text
                                            </p>
                                        </li>
                                        <li class="mb-5">
                                            <h6 class="">Option A</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Option_A
                                            </p>
                                        </li>
                                        <li class="mb-5">
                                            <h6 class="">Option B</h6>
                                            <p class="font-14 text-muted  ml-3">
                                                @examlog.Option_B
                                            </p>
                                        </li>
                                        <li class="mb-5">
                                            <h6 class="">Option E</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Option_E
                                            </p>
                                        </li>

                                    </ul>
                                </div>
                                <div class="col-md-12 col-lg-6">
                                    <ul class="list-unstyled">
                                        <li class="mb-5">
                                            <h6 class="">Refrence Image</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Refrence_Image
                                            </p>
                                        </li>
                                        <li class="mb-5">
                                            <h6 class="">Option C</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Option_C
                                            </p>
                                        </li>
                                        <li class="mb-5">
                                            <h6 class="">Option D</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Option_D
                                            </p>
                                        </li>
                                        <li class="mb-5">
                                            <h6 class="">Answer Choice</h6>
                                            <p class="font-14 text-muted ml-3">
                                                @examlog.Answer_Choice
                                            </p>
                                        </li>

                                    </ul>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-lg-4">
                    <div class="card">
                        <div class="card-body">
                            <h4 class="mt-0 header-title">Candidate Details</h4>
                            <p class="text-muted"><b>Jamb Number: @examlog.Jamb_No</b></p>
                            <p class="text-muted"><b>Subject(Course Code) @examlog.Subject:</b></p>
                            <p class="text-muted"><b>Department:@examlog.Department</b></p>
                            <p class="text-muted"><b>Semester:@examlog.Semester</b></p>
                            <p class="text-muted"><b>Session:@examlog.Session</b></p>
                            <p class="text-muted"><b>School:@examlog.School</b></p>
                            <p class="text-muted"><b>School Acronym:@examlog.School_Acronym</b></p>

                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-lg-12">
                    <div class="card">
                        <div class="card-body">

                            <div class="row">
                                <div class="col-md-12 col-lg-12">
                                    <table class="table table-striped table-bordered dt-responsive nowrap"
                                           style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                        <thead>
                                            <tr>
                                                <th>ANSWER_POINT</th>
                                                <th>OPTION_A_CHOICE</th>
                                                <th>OPTION_B_CHOICE</th>
                                                <th>OPTION_C_CHOICE</th>
                                                <th>OPTION_D_CHOICE</th>
                                                <th>OPTION_E_CHOICE</th>
                                            </tr>
                                        </thead>

                                        <tbody>

                                            <tr>
                                                <td>@examlog.Answer_Point</td>
                                                <td>@examlog.Option_A_Choice</td>
                                                <td>@examlog.Option_B_Choice</td>
                                                <td>@examlog.Option_C_Choice</td>
                                                <td>@examlog.Option_D_Choice</td>
                                                <td>@examlog.Option_E_Choice</td>

                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                                <div class="col-md-12 col-lg-12">
                                    <table class="table table-striped table-bordered dt-responsive nowrap"
                                           style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                        <thead>
                                            <tr>
                                                <th>IS_BONUS</th>
                                                <th>IS_OPTION_IMAGE</th>
                                                <th>ANS_FROM_STUDENT</th>
                                                <th>QUESTION_NO</th>
                                                <th>QUESTION_BATCH_NO</th>

                                            </tr>
                                        </thead>

                                        <tbody>

                                            <tr>
                                                <td>@examlog.Is_Bonus</td>
                                                <td>@examlog.Is_Option_Image</td>
                                                <td>@examlog.Ans_From_Student</td>
                                                <td>@examlog.Question_No</td>
                                                <td>@examlog.Question_Batch_No</td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <hr>
        }
    </div><!-- container -->

    <partial name="~/Views/Partials/_Footer.cshtml" />
</div>
<!-- end page content -->
}