                                            <span asp-validation-for="Option_D_Choice" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Option_E_Choice</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Option_E_Choice" placeholder="Option_E_Choice">
                                            <span asp-validation-for="Option_E_Choice" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Is_Bonus</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Is_Bonus" placeholder="Is_Bonus">
                                            <span asp-validation-for="Is_Bonus" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Is_Option_Image</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Is_Option_Image" placeholder="Is_Option_Image">
                                            <span asp-validation-for="Is_Option_Image" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>
