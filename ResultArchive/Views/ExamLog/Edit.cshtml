                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Option_A_Choice</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Option_A_Choice" placeholder="Option_A_Choice">
