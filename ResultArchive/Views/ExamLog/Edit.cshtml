                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">RegNumber</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="RegNumber" placeholder="RegNumber">
                                            <span asp-validation-for="RegNumber" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-lg-4 d-flex align-items-end">
                                    <div class="form-group row">
                                        <button type="submit" class="btn btn-primary ml-2">Update Logs</button>

                                    </div>
                                </div>
                                @if (ViewBag != null)
                                {
                                    <partial name="~/Views/Partials/_Alert.cshtml" />
                                }

                            </div>
                        </form>

                    </div><!--end card-body-->
                </div><!--end card-->
            </div><!--end col-->
        </div><!--end row-->

    </div><!-- container -->

    <partial name="~/Views/Partials/_Footer.cshtml" />
</div>
<!-- end page content -->