                                            <input class="form-control" type="text"  asp-for="Answer_Choice" placeholder="Answer Choice">
                                            <span asp-validation-for="Answer_Choice" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Answer Point</label>
