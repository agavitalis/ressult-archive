
@model ResultArchive.Models.ExamLog

@section LeftSidebar {
    <partial name="~/Views/Partials/_Sidebar.cshtml" />
}
<!-- Page Content-->
<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="mt-0 header-title">Edit Result</h4>
                        <p class="text-muted mb-4">
                            Here you edit <code class="highlighter-rouge">Results</code> registered in this system.
                        </p>

                        <form method="post">
                            <div class="row">
                                <input type="hidden" asp-for="Id" />
                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">SN</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="SN" placeholder="FirstName">
                                            <span asp-validation-for="SN" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Jamb No</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Jamb_No" placeholder="Jamb No">
                                            <span asp-validation-for="Jamb_No" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Subject</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Subject" placeholder="Subject">
                                            <span asp-validation-for="Subject" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Refrence Text</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Refrence_Text" placeholder="Refrence_Text">
                                            <span asp-validation-for="Refrence_Text" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>


                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Refrence Image</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Refrence_Image" placeholder="CVScore">
                                            <span asp-validation-for="Refrence_Image" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Question</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Question" placeholder="Question">
                                            <span asp-validation-for="Question" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-md-4">
                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Option A</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Option_A" placeholder="Option A">
                                            <span asp-validation-for="Option_A" class="text-danger"></span>
