                                    <div class="form-group row">
                                        <label for="example-text-input" class="col-sm-4 col-form-label text-right">Option E</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text"  asp-for="Option_E" placeholder="Option D">
                                            <span asp-validation-for="Option_E" class="text-danger"></span>
